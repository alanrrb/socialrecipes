<!DOCTYPE html>
<html>
<head>
  <title>Socialrecipes</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'shared/top' %>
<div class="container">
  <div class="row">
    <div class="col-md-3">
      <%= render 'shared/categories'%>
    </div>
    <div class="col-md-9">
    <%= yield %>
    </div>
  </div>
</div>
<%= render 'shared/footer' %>
</body>
</html>
